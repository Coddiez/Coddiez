                        android:layout_weight="45"
                        android:fontFamily="@font/productsansmedium"
                        android:paddingLeft="15dp"
                        android:paddingTop="10dp"
                        android:paddingBottom="10dp"
                        android:text="Name"
                        android:textColor="@color/textcolorNonSelected"
                        android:textSize="18dp" />

                    <TextView
                        android:id="@+id/start"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="20"
                        android:fontFamily="@font/productsansmedium"
                        android:paddingTop="10dp"
                        android:paddingBottom="10dp"
                        android:text="Start"
                        android:textColor="@color/textcolorNonSelected"
                        android:textSize="18dp" />


                    <TextView
                        android:id="@+id/end"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="20"
                        android:fontFamily="@font/productsansmedium"
                        android:paddingTop="10dp"
                        android:paddingBottom="10dp"
                        android:text="End"
                        android:textColor="@color/textcolorNonSelected"
                        android:textSize="18dp" />


                </TableRow>

                <TableRow
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">

                    <LinearLayout
                        android:id="@+id/loadingContestData"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:layout_weight="1"
                        android:orientation="vertical"
                        android:visibility="visible">

                        <ProgressBar
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:layout_margin="20dp"
                            android:indeterminate="true"
                            android:indeterminateTint="@color/whitesecondary"
                            android:indeterminateTintMode="src_atop" />

                        <View
                            android:layout_width="match_parent"
                            android:layout_height="1dp"
                            android:background="@color/whitesecondary"
                            app:layout_constraintBottom_toBottomOf="@id/dataLinearlayout" />

                    </LinearLayout>


                    <androidx.recyclerview.widget.RecyclerView
                        android:id="@+id/contestDetailsList"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:visibility="gone"
                        tools:listitem="@layout/contest_data_layout" />


                    <LinearLayout
                        android:id="@+id/errorInContestDataLayout"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:orientation="vertical"
                        android:visibility="gone">

                        <TextView
                            android:id="@+id/errorInContestDataText"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_weight="1"
                            android:fontFamily="@font/productsansmedium"
                            android:paddingTop="15dp"
                            android:paddingBottom="15dp"
                            android:text="No data available"
                            android:textAlignment="center"
                            android:textColor="@color/whitesecondary"
                            android:textSize="12dp"
                            android:visibility="visible" />

                        <View
                            android:layout_width="match_parent"
                            android:layout_height="1dp"
                            android:background="@color/whitesecondary"
                            app:layout_constraintBottom_toBottomOf="@id/dataLinearlayout" />

                    </LinearLayout>


                </TableRow>

            </TableLayout>


