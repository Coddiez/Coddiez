<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/bgforDark"
    tools:context=".MainActivity2">


    <androidx.core.widget.NestedScrollView
        android:id="@+id/fir"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:visibility="visible"
        app:layout_constraintTop_toTopOf="parent">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">


            <!--            Heading -->
            <TextView
                android:id="@+id/contests"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="27dp"
                android:layout_marginTop="30dp"
                android:fontFamily="@font/productsansmedium"
                android:text="Contests"
                android:textColor="#DEFFFFFF"
                android:textSize="30dp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />


            <!--            Selection of timing and platform-->
            <TableLayout
                android:id="@+id/tableListForOngoingUpcomingChoice"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="30dp"
                android:background="@drawable/white_bg"
                android:orientation="horizontal"
                app:layout_constraintStart_toStartOf="@+id/contests"
                app:layout_constraintTop_toBottomOf="@+id/contests">


                <TableRow>

                    <Button
                        android:id="@+id/ongoing"
                        android:layout_width="wrap_content"
                        android:layout_height="24dp"
                        android:layout_marginStart="3dp"
                        android:layout_marginTop="3dp"
                        android:layout_marginBottom="3dp"
                        android:background="@drawable/grey_bg"
                        android:fontFamily="@font/productsansregular"
                        android:paddingStart="12dp"
                        android:paddingEnd="12dp"
                        android:text="Ongoing"
                        android:textAllCaps="false"
                        android:textColor="@color/whitemain"
                        android:textSize="15dp"
                        app:backgroundTint="@null"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintHorizontal_bias="0.5"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent" />

                    <Button
                        android:id="@+id/upcoming"
                        android:layout_width="wrap_content"
                        android:layout_height="24dp"
                        android:layout_marginStart="3dp"
                        android:layout_marginTop="3dp"
                        android:layout_marginEnd="3dp"
                        android:layout_marginBottom="3dp"
                        android:background="@null"
                        android:fontFamily="@font/productsansregular"
                        android:paddingStart="12dp"
                        android:paddingEnd="12dp"
                        android:text="Upcoming"
                        android:textAllCaps="false"
                        android:textColor="@color/textcolorNonSelected"
                        android:textSize="15dp"
                        app:backgroundTint="@null"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintHorizontal_bias="0.5"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent" />
                </TableRow>

            </TableLayout>

